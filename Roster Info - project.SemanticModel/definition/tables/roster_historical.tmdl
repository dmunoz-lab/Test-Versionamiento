table roster_historical
	lineageTag: a9b6147d-b5ee-4180-b153-8acb1b7be889

	measure previous_month = ```
			
			    var _currentMonth = MONTH(MAX(Dim_Calendar[Date]))
			    var _currentYear = YEAR(MAX(Dim_Calendar[Date]))
			    var _previousMonth = IF(_currentMonth = 1, 12, _currentMonth-1)
			    var _previousYear = IF(_currentMonth=1, _currentYear-1, _currentYear)
			return
			    CALCULATE(COUNT(roster_historical[ID Number]),
			            MONTH(Dim_Calendar[Date]) = _previousMonth, YEAR(Dim_Calendar[Date]) = _previousYear, REMOVEFILTERS(Dim_Calendar[Month]))
			        
			```
		formatString: 0
		lineageTag: 9deeafe0-bab7-4430-8682-0de00378e52a

		changedProperty = IsHidden

	measure 'Actual Month' = ```
			
			CALCULATE(COUNT(roster_historical[ID Number]),
			    MONTH(roster_historical[Roster date]) = MONTH(MAX(roster_historical[Roster date])), 
			    YEAR(roster_historical[Roster date]) = YEAR(MAX(roster_historical[Roster date])))
			```
		formatString: 0
		lineageTag: cc475f23-c626-432c-9e5e-9dcacefe5fb8

		changedProperty = IsHidden

	measure Difference = [Actual Month] - [previous_month]
		formatString: 0
		lineageTag: a8bd061d-fc11-4dc8-82d0-5d78cb5af265

		changedProperty = IsHidden

	measure currentMonth = YEAR(MAX(roster_historical[Roster date]))
		formatString: 0
		lineageTag: 9e2d41f8-056f-41a6-8fbe-1641fdd523f1

	measure '0. Total Headcount' = IF(COUNT(roster_historical[ID Number])=0,0,COUNT(roster_historical[ID Number]))
		formatString: 0
		lineageTag: 02339327-215d-41fa-8a17-7b41e772ecea

		annotation PBI_FormatHint = {"isDecimal":true}

	measure '0. Total Unplaceable BORRAR' = CALCULATE(COUNT(roster_historical[ID Number]),OR(roster_historical[status] = "Not available",roster_historical[status]  = "Free Floater"))
		formatString: 0
		lineageTag: 4708a61f-a3aa-48e2-baa3-6d5dca39e93e

	measure '2. % Unpla vs. Active' = [0. Total Unplaceable BORRAR] / CALCULATE(COUNT(roster_historical[ID Number]), roster_historical[status] = "Active")
		formatString: 0.0%;-0.0%;0.0%
		lineageTag: ab06f1e8-daee-4afe-8b7c-a989595dccce

